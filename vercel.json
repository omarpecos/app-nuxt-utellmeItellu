{
  "version": 2,
  "routes": [
    {
      "src": "/api/jokes",
      "dest": "/api/jokes.ts"
    },
    {
      "src": "/api/countries",
      "dest": "/api/countries.ts"
    }
  ],
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {}
    }
  ]
}
